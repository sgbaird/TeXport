Cell[BoxData[{
 RowBox[{"Clear", "[", "TeXport", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "TeXport", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Print", "\[Rule]", "False"}], ",", 
     RowBox[{"Export", "\[Rule]", "False"}], ",", 
     RowBox[{"Folder", "\[Rule]", "\"\<.\>\""}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TeXport", "[", 
   RowBox[{"fname_", ",", "eqn_", ",", "keyvalues_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "folder", ",", "printQ", ",", "exportQ", ",", "fnametxt", ",", 
      "fnametex", ",", "fpathtxt", ",", "fpathtex", ",", "txt", ",", "keys", 
      ",", "values"}], "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"PARSE", " ", "options"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"printQ", ",", "exportQ", ",", "folder"}], "}"}], "=", 
      RowBox[{"OptionValue", "[", 
       RowBox[{"{", 
        RowBox[{
        "\"\<Print\>\"", ",", "\"\<Export\>\"", ",", "\"\<Folder\>\""}], 
        "}"}], "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"MAKE", " ", "file", " ", "path", " ", "strings"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"fnametxt", "=", 
      RowBox[{"fname", "<>", "\"\<.txt\>\""}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"fnametex", "=", 
      RowBox[{"fname", "<>", "\"\<.tex\>\""}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"fpathtxt", ",", "fpathtex"}], "}"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"FileNameJoin", "[", 
         RowBox[{"{", 
          RowBox[{"folder", ",", "#"}], "}"}], "]"}], "&"}], "/@", 
       RowBox[{"{", 
        RowBox[{"fnametxt", ",", "fnametex"}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"MAKE", " ", "the", " ", "sentence"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"sentence", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "@", "keyvalues"}], ">", "0"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"keys", ",", "values"}], "}"}], "=", "keyvalues"}], ";", 
         RowBox[{"\"\<\n\>\"", "<>", 
          RowBox[{"KeyValueToSentence", "[", 
           RowBox[{"keys", ",", "values"}], "]"}]}]}], ",", "\"\<\>\""}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"txt", "=", 
      RowBox[{
      "\"\<\\\\begin{equation} \\\\label{eq:\>\"", "<>", "fname", "<>", 
       "\"\<}\n\t\>\"", "<>", 
       RowBox[{"ToString", "@", 
        RowBox[{"TeXForm", "[", "eqn", "]"}]}], "<>", 
       "\"\<\n\\\\end{equation}\>\"", "<>", "sentence"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"PRINT", " ", "or", " ", "EXPORT", " ", "the", " ", "result"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{"exportQ", ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Export", "[", 
         RowBox[{"fpathtxt", ",", "txt"}], "]"}], ";", 
        RowBox[{"RenameFile", "[", 
         RowBox[{"fpathtxt", ",", "fpathtex", ",", 
          RowBox[{"OverwriteTarget", "\[Rule]", "True"}]}], "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"##", "&"}], "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{"printQ", ",", "\[IndentingNewLine]", 
       RowBox[{"Print", "@", "txt"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"##", "&"}], "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     "txt"}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
