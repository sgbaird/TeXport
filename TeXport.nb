Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"KeyToSubjectPredicate", "::", "usage"}], "=", 
    "\"\<KeyToSubjectPredicate[keys] takes a set of keys (e.g. {a,b,c}) and \
outputs a predicate of these keys (e.g. \\\"where $a$, $b$, and $c$ represent \
\\\")\>\""}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"ValueToObject", "::", "usage"}], "=", 
    "\"\<ValueToObject[values] converts a set of values (e.g. \
{\\\"apples\\\",\\\"bananas\\\",\\\"cantaloupe\\\"} into the object of a \
sentence (e.g. \\\"apples, bananas, and cantaloupe, respectively.\\\")\>\""}],
    ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"KeyValueToSentence", "::", "usage"}], "=", 
    "\"\<KeyValueToSentence[keys,values] concatenates \
KeyToSubjectPredicate[keys] with ValueToObject[values] as in \\\"where $a$, \
$b$, and $c$ represent apples, bananas, and cantaloupe, \
respectively.\\\"\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"TeXport", "::", "usage"}], "=", 
    "\"\<To set default options for later calls to TeXport, use e.g. \
SetOptions[TeXport,Export\[Rule]True]\nTeXport[fname,eqn,keyvalues] returns a \
TeX-formatted sentence corresponding to an expression (eqn) and a definition \
of variables (keyvalues) that immediately follows in a sentence format. E.g. \
\\\"\\\\begin{equation} \
\\\\label{eq:texport-example}\\n\\ta+b=c\\n\\\\end{equation}\\nwhere $a$, \
$b$, and $c$ represent apples, bananas, and cantaloupe, respectively.\\\"\n\
TeXport[fname,eqn,keyvalues,Print\[Rule]True] prints a TeX-formatted \
expression to $Output with key-value pair definitions.\n\
TeXport[fname,eqn,,Print\[Rule]True] prints a TeX-formatted expression to \
$Output without key-value pairs.\n\
TeXport[fname,eqn,keyvalues,Export\[Rule]True] exports a TeX-formatted \
expression to <fname>.tex.\n\
TeXport[fname,eqn,keyvalues,Export\[Rule]True,Folder\[Rule]\\\"equations\\\"] \
exports a TeX-formatted expression to equations\\\\<fname>.tex.\n\>\""}], 
   ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"OpenWriteTeX", "::", "usage"}], "=", 
    "\"\<OpenWriteTeX[file] opens file such that writing to it will have an \
infinite page width and the UTF8 character encoding. This is especially \
useful for exporting full derivations (i.e. multiple expressions) and \
intermediate sentences to a .tex file. By default, file is overwritten when \
OpenWriteTeX is called.\nOpenWriteTeX[] opens a temporary file with the same \
properties as above.\nOpenWriteTeX[file,overwriteQ\[Rule]False] opens a file \
without overwriting (i.e. append to file).\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"WL", "::", "usage"}], "=", 
    "\"\<WL stands for and is a wrapper to WriteLine (MMA built-in function), \
but is shorter and easier to call. A default file can be set by using \
file=OpenWriteTeX@fpath and then SetOptions[WL,File\[Rule]file]. After \
setting a default file, WL@x and x//WL allow for less-obtrusive exporting \
code (i.e. generally more readable).\nWL[x,File\[Rule]file] writes x to file.\
\nWL[x,File\[Rule]file,Print\[Rule]True] writes x to file and prints to \
$Output.\nWL[x] writes x to the default file specified by SetOptions[WL,File\
\[Rule]file].\>\""}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"MathText", "::", "usage"}], "=", 
   "\"\<MathText[x] converts the non-string expression x to a TeX version \
($x$).\>\""}], ";"}]}], "Input",
 InitializationCell->True,
